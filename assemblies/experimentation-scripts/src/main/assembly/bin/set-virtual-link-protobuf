#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 2 ]
then
	echo "Usage $0 PROPERTIES_FILE SECRET_RESERVATION_KEYS SOURCE_NODE_URN TARGET_NODE_URN"
	echo "Example: $0 live.properties urn:wisebed:uzl1:,D2C88AEE416EDD2FD4038BC1252A8900 urn:wisebed:uzl1:0xf859 urn:wisebed:uzl1:0xf860"
	exit 1
fi

java -Dtestbed.secretreservationkeys=$2 -Dtestbed.vlink.from=$3 -Dtestbed.vlink.to=$4 -jar $BASEDIR/../lib/tr.wisebed-cmdline-client-${project.version}-onejar.jar -p $1 -f $BASEDIR/../scripts/set-virtual-link-protobuf.java
